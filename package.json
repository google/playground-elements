{
  "name": "playground-elements",
  "version": "0.14.7",
  "description": "Serverless coding environments for the web",
  "homepage": "https://github.com/google/playground-elements#readme",
  "repository": "github:google/playground-elements",
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "type": "module",
  "module": "playground-ide.js",
  "main": "playground-ide.js",
  "scripts": {
    "prepare": "if [ -f './web-test-runner.config.js' ]; then cd node_modules/puppeteer && PUPPETEER_PRODUCT=firefox node install.js; fi",
    "prepack": "npm run clean && npm run build && npm run update-version-module",
    "update-version-module": "node scripts/update-version-module.js",
    "clean": "git clean -dXn | sed 's/^Would remove //' | grep -v node_modules | xargs rm -rf",
    "nuke": "npm run clean && rm -rf node_modules && npm install",
    "upgrade": "rm -rf node_modules package-lock.json && npm install",
    "build": "npm run build:themes && npm run build:lib && npm run build:typescript && npm run bundle && npm run build:configurator",
    "build:lib": "tsc --build",
    "build:themes": "node scripts/theme-generator.js && rollup -c rollup.config.styles.js",
    "build:configurator": "rollup -c rollup.config.configurator.js",
    "build:typescript": "rollup -c rollup.config.typescript.js",
    "bundle": "rollup -c rollup.config.js",
    "test": "wtr && playwright test",
    "lint": "eslint .",
    "watch": "npm run build:lib -- --watch & NO_TERSER=1 rollup -c rollup.config.js -w",
    "serve": "web-dev-server --node-resolve --watch --open=configurator/",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "files": [
    "_codemirror/",
    "images/",
    "index.{js,d.ts,d.ts.map}",
    "playground-code-editor.{js,d.ts,d.ts.map}",
    "playground-connected-element.{js,d.ts,d.ts.map}",
    "playground-file-editor.{js,d.ts,d.ts.map}",
    "playground-file-system-controls.{js,d.ts,d.ts.map}",
    "playground-ide.{js,d.ts,d.ts.map}",
    "playground-preview.{js,d.ts,d.ts.map}",
    "playground-project.{js,d.ts,d.ts.map}",
    "playground-tab-bar.{js,d.ts,d.ts.map}",
    "playground-service-worker-proxy.html",
    "playground-service-worker.js",
    "playground-typescript-worker.js",
    "playground-styles.{css,js,d.ts,d.ts.map}",
    "internal/**/*.{js,d.ts,d.ts.map}",
    "shared/**/*.{js,d.ts,d.ts.map}",
    "themes/*.css",
    "themes/*.css.{js,d.ts,d.ts.map}"
  ],
  "devDependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@material/mwc-dialog": "^0.25.1",
    "@playwright/test": "^1.15.1",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/semver": "^7.3.6",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@web/dev-server": "^0.1.4",
    "@web/test-runner": "^0.13.4",
    "@web/test-runner-playwright": "^0.8.0",
    "@web/test-runner-puppeteer": "^0.10.0",
    "clean-css": "^5.1.2",
    "codemirror": "=5.63.0",
    "codemirror-grammar-mode": "^0.1.10",
    "es-module-lexer": "^0.9.3",
    "eslint": "^7.31.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "google_modes": "git+https://github.com/codemirror/google-modes.git#e4725332b9bba4c3418f29a28f3042799d559e76",
    "playwright": "^1.15.1",
    "prettier": "^2.0.5",
    "rollup": "^2.21.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-lit-css": "^3.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-summary": "^1.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "^7.3.5",
    "tsc-watch": "^4.2.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@material/mwc-button": "^0.25.1",
    "@material/mwc-icon-button": "^0.25.1",
    "@material/mwc-linear-progress": "^0.25.1",
    "@material/mwc-list": "^0.25.1",
    "@material/mwc-menu": "^0.25.1",
    "@material/mwc-textfield": "^0.25.1",
    "@types/codemirror": "^5.60.0",
    "comlink": "=4.3.1",
    "lit": "^2.0.0",
    "tslib": "^2.0.3",
    "vscode-languageserver": "^7.0.0"
  }
}
